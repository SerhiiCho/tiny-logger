{
    "name": "serhii/tiny-logger",
    "description": "Light weight composer package for file logging in PHP7.",
    "homepage": "https://github.com/SerhiiCho/tiny-logger",
    "keywords": ["logger", "library"],
    "type": "library",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-curl": "*",
        "curl/curl": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/var-dumper": "^4.2",
        "phpstan/phpstan": "0.12.x-dev",
        "squizlabs/php_codesniffer": "^4.0",
        "sandfoxme/private-access": "^1.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SerhiiCho",
            "email": "serhiicho@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Serhii\\TinyLogger\\": "src"
        },
        "files": [
            "logger.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Serhii\\Tests\\": "ests"
        }
    },
    "minimum-stability": "dev"
}
