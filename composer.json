{
    "name": "serhii/tiny-logger",
    "description": "Light weight composer package for file logging in PHP7.",
    "homepage": "https://github.com/SerhiiCho/tiny-logger",
    "keywords": ["logger", "library"],
    "type": "library",
    "require": {
        "php": "^7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "SerhiiCho",
            "email": "serhiicho@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Serhii\\TinyLogger\\": "src"
        },
        "files": [
            "logger.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Serhii\\Tests\\": "ests"
        }
    },
    "minimum-stability": "dev"
}
